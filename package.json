{
  "name": "lambdalith-router",
  "version": "1.0.0",
  "description": "A minimal platform-agnostic routing library, made for use with Lambdaliths",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && npm run bundle",
    "bundle": "esbuild src/index.ts --bundle --minify --sourcemap --outfile=dist/index.esm.js --format=esm && esbuild src/index.ts --bundle --minify --sourcemap --outfile=dist/index.cjs.js --format=cjs"
  },
  "devDependencies": {
    "esbuild": "^0.27.2",
    "typescript": "^5.9.3"
  },
  "license": "MIT"
}
